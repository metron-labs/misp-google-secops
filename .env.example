# MISP Configuration
# URL of your MISP instance
MISP_URL=https://localhost

# Your MISP API Key (find this in MISP Web UI -> Administration -> List Auth Keys)
MISP_API_KEY=misp_api_key 

# Verify SSL (set to false if using self-signed certs)
MISP_VERIFY_SSL=false

# Google SecOps Configuration
# Path to your Service Account JSON key file (inside the container or local path)
GOOGLE_SA_CREDENTIALS=/app/credentials.json

# Your Google SecOps Customer ID
GOOGLE_CUSTOMER_ID=your_customer_id

# Project ID (Optional, sometimes needed for logging)
GOOGLE_PROJECT_ID=your_project_id

# Polling Settings
# How often to check for new attributes (in seconds)
FETCH_INTERVAL=3600

#Test mode
TEST_MODE=true
MAX_TEST_EVENTS=3

# Batch sizes
FETCH_PAGE_SIZE=100
FORWARDER_BATCH_SIZE=100
